package iforMoon;

public class Testfor {

	public static void main(String[] args) {
		
		for (int i = 2; i < 10; i++) {
			
			for (int j = 1; j < 10; j++) {
				if ( i == 4 ) {
					System.out.println(i + " X " + j + " = " + (i * j + 1));
				} else {
					System.out.println(i + " X " + j + " = " + i * j);
					}
			}
				
			
			}
		}

	}
