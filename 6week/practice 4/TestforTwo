package iforMoon;

public class Testfor {

	public static void main(String[] args) {
		
		for (int i = 2; i < 10; i++) {
			
			for (int j = 1; j < 10; j++) {
				int result = 0;
				
				if ( i == 4 ) {
					result = i * j + 1;
				} else {
					result = i * j;
					}
				System.out.println(i + " X " + j + " = " + result);
			}
		}
	}

}
